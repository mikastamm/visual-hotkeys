[
  {
    "url": "google.com\/search.*",
    "hotkeys": [
      {
        "_name": "Top Level Search Result",
        "keystrokes": {
          "sequence": {
            "type": "digits"
          }
        },
        "displayXpath": "//div[@class=\"MjjYud\"]",
        "clickXpath": "//a",
        "injectionType": "absoluteBefore"
      }
    ]
  },
  {
    "url": "github.com\/.*",
    "hotkeys": [
      {
        "_name": "User Settings",
        "keystrokes": {
          "preset": "menu"
        },
        "displayXpath": "//details[starts-with(@data-feature-preview-indicator-src, '/users/')]/..",
        "clickXpath": "/details/summary",
        "injectionType": "inlineAfter"
      }
    ]
  }
]